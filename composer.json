{
    "name": "hypervel/prompts",
    "type": "library",
    "description": "Add beautiful and user-friendly forms to your command-line applications.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "prompts",
        "swoole",
        "hypervel"
    ],
    "autoload": {
        "psr-4": {
            "Hypervel\\Prompts\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "composer-runtime-api": "^2.2",
        "symfony/console": "^6.2|^7.0",
        "nunomaduro/termwind": "^2.0"
    },
    "require-dev": {
        "hyperf/collection": "3.2.x-dev",
        "mockery/mockery": "^1.5"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
